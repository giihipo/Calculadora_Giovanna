alert("digite dois números");

let operador = prompt("que operação você deseja fazer?")

let numeroUm = prompt("digite o primeiro número");

let numeroDois = prompt("digite o segundo número");

if(operador == "+"){
  console.log("O resultado da soma é:", numeroUm + numeroDois);

}else if(operador == "-"){
  console.log("O resultado da subtração é:", numeroUm - numeroDois);
  
}else if(operador == "/"){
  console.log("O resultado da divisão é:", numeroUm / numeroDois);

}else if (operador == "*"){
   console.log("O resultado da multiplicação é:", numeroUm * numeroDois)         
}
